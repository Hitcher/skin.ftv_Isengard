<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Top Rated Movies</name>
    <match>one</match>
    <rule field="rating" operator="is">
        <value>10</value>
    </rule>
    <rule field="rating" operator="lessthan">
        <value>10</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">rating</order>
</smartplaylist>