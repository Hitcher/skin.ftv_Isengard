<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- osdvideosettings -->
	<defaultcontrol always="true">5</defaultcontrol>
	<controls>
		<control type="group">
			<top>655</top>
			<animation effect="fade" start="100" end="0" time="200" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
			<!-- Background -->
			<control type="image">
				<width>1920</width>
				<height>425</height>
				<texture>osd/mask.png</texture>
			</control>
			<!-- Heading label -->
			<control type="label" id="2">
				<left>92</left>
				<top>20</top>
				<height>36</height>
				<font>Bold30</font>
				<textcolor>ffdadada</textcolor>
			</control>
			<!-- Grouplist -->
			<control type="grouplist" id="5">
				<left>87</left>
				<top>60</top>
				<width>805</width>
				<height>348</height>
				<itemgap>0</itemgap>
				<onleft>noop</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<orientation>vertical</orientation>
				<scrolltime tween="sine" easing="out">240</scrolltime>
			</control>
			<!-- Default button -->
			<control type="button" id="7">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
			</control>
			<!-- Default radiobutton -->
			<control type="radiobutton" id="8">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<radioposx>708</radioposx>
				<radiowidth>100</radiowidth>
				<radioheight>58</radioheight>
				<textureradioonfocus>dialogs/RadioOnFO.png</textureradioonfocus>
				<textureradioonnofocus>dialogs/RadioOnNF.png</textureradioonnofocus>
				<textureradioofffocus>dialogs/RadioOffFO.png</textureradioofffocus>
				<textureradiooffnofocus>dialogs/RadioOffNF.png</textureradiooffnofocus>
			</control>
			<!-- Default spincontrolex -->
			<control type="spincontrolex" id="9">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
			</control>
			<!-- Default sliderex -->
			<control type="sliderex" id="13">
				<height>58</height>
				<font>Light32</font>
				<textoffsetx>5</textoffsetx>
				<textcolor>ffffffff</textcolor>
				<shadowcolor>ff000000</shadowcolor>
				<focusedcolor>ffffc04b</focusedcolor>
				<disabledcolor>88ffffff</disabledcolor>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<sliderwidth>200</sliderwidth>
				<sliderheight>11</sliderheight>
			</control>
		</control>
	</controls>
</window>