<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Viewtype56">
		<control type="group">
			<visible>Control.IsVisible(56)</visible>
			<!-- List -->
			<control id="56" type="fixedlist">
				<left>-238</left>
				<top>242</top>
				<width>2200</width>
				<height>576</height>
				<onup>Back</onup>
				<ondown>1001</ondown>
				<onleft>Back</onleft>
				<onright>noop</onright>
				<onright condition="!Window.IsVisible(1103)">SetProperty(BounceRight,1,Home)</onright>
				<preloaditems>2</preloaditems>
				<focusposition>1</focusposition>
				<viewtype label="31161">list</viewtype>
				<orientation>horizontal</orientation>
				<defaultcontrol always="true">0</defaultcontrol>
				<scrolltime easing="inout" tween="sine">300</scrolltime>
				<visible>!IsEmpty(Container.PluginName) | [!Container.Content(movies) + !Container.Content(sets) + !Container.Content(tvshows) + !Container.Content(seasons) + !Container.Content(episodes) + !Container.Content(songs)]</visible>
				<animation condition="Window.IsVisible(1103)" reversible="false" type="Conditional">
					<effect easing="out" end="-20,0" start="0,0" time="60" tween="sine" type="slide" />
					<effect delay="80" easing="out" end="20,0" start="0,0" time="120" tween="sine" type="slide" />
				</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + IsEmpty(Window(home).Property(FocusRow)) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="193,10" time="540" tween="sine">WindowOpen</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + StringCompare(Window(home).Property(FocusRow),1_2) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="497,-175" time="540" tween="sine">WindowOpen</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + StringCompare(Window(home).Property(FocusRow),2_2) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="497,195" time="540" tween="sine">WindowOpen</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + StringCompare(Window(home).Property(FocusRow),1_3) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="497,-246" time="540" tween="sine">WindowOpen</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + StringCompare(Window(home).Property(FocusRow),2_3) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="497,10" time="540" tween="sine">WindowOpen</animation>
				<animation condition="IsEmpty(Window(home).Property(MainMenuFocus)) + StringCompare(Window(home).Property(FocusRow),3_3) + Window.Previous(home)" easing="inout" effect="slide" end="0,0" start="497,168" time="540" tween="sine">WindowOpen</animation>
				<itemlayout height="576" width="436">
					<control type="group">
						<animation center="250,288" condition="true" effect="zoom" end="85" start="85">Conditional</animation>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<aspectratio align="center">scale</aspectratio>
							<texture background="true" fallback="views/square_fallback.png">$INFO[ListItem.Icon]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayHD.png</texture>
							<visible>IntegerGreaterThan(ListItem.VideoResolution,719)</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayWatched.png</texture>
							<visible>StringCompare(ListItem.Overlay,OverlayWatched.png)</visible>
						</control>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="66,66,51,51">views/shadow_nf.png</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture border="2">views/season_overlay.png</texture>
							<visible>!StringCompare(ListItem.Label,..) + !SubString(ListItem.Icon,Default)</visible>
						</control>
						<control type="label">
							<left>18</left>
							<top>447</top>
							<width>446</width>
							<height>42</height>
							<font>Bold44</font>
							<scroll>false</scroll>
							<textcolor>ffffffff</textcolor>
							<shadowcolor>88000000</shadowcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>!StringCompare(ListItem.Label,..)</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture>views/OverlayNF.png</texture>
						</control>
					</control>
				</itemlayout>
				<focusedlayout height="576" width="436">
					<control type="button">
						<texturefocus>-</texturefocus>
						<texturenofocus>-</texturenofocus>
						<onfocus>ClearProperty(TVShowID,home)</onfocus>
						<onfocus>ClearProperty(PercentRemaining,home)</onfocus>
						<onfocus condition="ListItem.IsResumable">SetProperty(PercentRemaining,$VAR[PercentRemaining],home)</onfocus>
					</control>
					<control type="group">
						<animation center="250,288" easing="inout" effect="zoom" end="100" start="85" time="300" tween="sine">Focus</animation>
						<animation center="250,288" condition="Container(56).OnNext | Container(56).OnPrevious" easing="inout" effect="zoom" end="85" start="100" time="300" tween="sine">UnFocus</animation>
						<animation center="250,288" condition="Control.HasFocus(4000)" easing="inout" effect="zoom" end="85" start="100" time="300" tween="sine">Conditional</animation>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<aspectratio align="center">scale</aspectratio>
							<texture background="true" fallback="views/wide_fallback.png">$INFO[ListItem.Icon]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayHD.png</texture>
							<visible>IntegerGreaterThan(ListItem.VideoResolution,719)</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayWatched.png</texture>
							<visible>StringCompare(ListItem.Overlay,OverlayWatched.png)</visible>
						</control>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="51">views/shadow_fo.png</texture>
							<animation center="auto" condition="Control.HasFocus(56) | Container(56).OnNext | Container(56).OnPrevious" easing="inout" effect="fade" end="100" start="0" time="300" tween="sine">Focus</animation>
							<animation center="auto" condition="Container(56).OnNext | Container(56).OnPrevious" easing="inout" effect="fade" end="0" start="100" time="300" tween="sine">UnFocus</animation>
						</control>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="66,66,51,51">views/shadow_nf.png</texture>
							<animation center="auto" condition="Control.HasFocus(56)" easing="inout" effect="fade" end="0" start="100" time="300" tween="sine">Focus</animation>
							<animation center="auto" condition="Control.HasFocus(56)" easing="inout" effect="fade" end="100" start="0" time="300" tween="sine">UnFocus</animation>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture border="2">views/season_overlay.png</texture>
							<visible>!StringCompare(ListItem.Label,..) + !SubString(ListItem.Icon,Default)</visible>
						</control>
						<control type="label">
							<left>18</left>
							<top>447</top>
							<width>446</width>
							<height>42</height>
							<font>Bold44</font>
							<scroll>false</scroll>
							<textcolor>ffffffff</textcolor>
							<shadowcolor>88000000</shadowcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>!StringCompare(ListItem.Label,..)</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture>views/OverlayNF.png</texture>
							<animation center="auto" condition="Control.HasFocus(56)" easing="inout" effect="fade" end="0" start="100" time="300" tween="sine">Focus</animation>
							<animation center="auto" condition="Control.HasFocus(56)" easing="inout" effect="fade" end="100" start="0" time="300" tween="sine">UnFocus</animation>
						</control>
					</control>
				</focusedlayout>
			</control>
		</control>
	</include>
</includes>